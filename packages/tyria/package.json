{
  "name": "tyria",
  "version": "0.0.1",
  "description": "Raster tile map viewer",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup src/index.ts src/image-fetch-worker.ts --format esm --minify",
    "dev": "tsup src/index.ts src/image-fetch-worker.ts --format esm --watch --sourcemap & live-server --ignore=src --mount=/dist:./dist dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darthmaim/tyria.git"
  },
  "keywords": [
    "map",
    "raster map",
    "tiles",
    "raster map tiles",
    "slippy map",
    "interactive map"
  ],
  "author": "darthmaim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darthmaim/tyria/issues"
  },
  "homepage": "https://github.com/darthmaim/tyria#readme",
  "devDependencies": {
    "live-server": "^1.2.2",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  }
}
